# Jazzapedia Environment Variables
# Copy this file to .env and customize for your environment

# Obsidian vault paths for artist discovery integration
# These are used by the scraper service in docker-compose.yml
ARTISTS_DIR=/path/to/your/obsidian/vault/Artists
PORTRAITS_DIR=/path/to/your/obsidian/vault/ArtistPortraits

# Obsidian archives (where daily WWOZ logs are written)
OBSIDIAN_ARCHIVES=/path/to/your/obsidian/vault/wwoztracker
